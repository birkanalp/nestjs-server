{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@dto": ["./src/dto"],
      "@dto/*": ["./src/dto/*"],

      "@entity": ["./src/entity"],
      "@entity/*": ["./src/entity/*"],

      "@guard": ["./src/guard"],
      "@guard/*": ["./src/guard/*"],

      "@model": ["./src/model"],
      "@model/*": ["./src/model/*"],

      "@module": ["./src/module"],
      "@module/*": ["./src/module/*"],

      "@service": ["./src/service"],
      "@service/*": ["./src/service/*"],

      "@constant": ["./src/constant"],
      "@constant/*": ["./src/constant/*"],

      "@util": ["./src/util"],
      "@util/*": ["./src/util/*"],

      "@decorator": ["./src/decorator"],
      "@decorator/*": ["./src/decorator/*"],

      "@interceptor": ["./src/interceptor"],
      "@interceptor/*": ["./src/interceptor/*"]
    }
  }
}
